<div class="my-2 my-lg-0 inner-content-wrapper review-form-wrapper">
  <%= form_for(@review, :url => edit_review_path({:user_id => @user.id, :role => @role}), :html => { :method => :put }) do |f| %>
    <%= render :partial => 'users/review_form', :locals => { :f => f, :review => @review } %>
  <% end %>
</div>
$("#flash-message").html("<%= j (render 'layouts/flash_message') %>");

if(<%= j @success %>){
  if("<%= j @toggle %>" == "on"){
    $('#toggle-active-btn').text('Deactivate');
    $('#active-status').text('Active');
  }else{
    $('#toggle-active-btn').text('Activate');  
    $('#active-status').text('Inactive');
  }
}